{
  "name": "redux-tab-connect",
  "version": "1.0.0",
  "description": "A lightweight Redux middleware to sync state across browser tabs using BroadcastChannel API",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json",
    "build:watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "redux",
    "tab-sync",
    "browser-tabs",
    "state-sync",
    "broadcast-channel",
    "multi-tab",
    "cross-tab",
    "redux-middleware",
    "state-management",
    "real-time",
    "synchronization",
    "react",
    "typescript",
    "javascript",
    "frontend",
    "web",
    "middleware",
    "redux-toolkit",
    "browser",
    "tabs"
  ],
  "author": "Asad",
  "license": "MIT",
  "dependencies": {
    "broadcast-channel": "^5.5.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/uuid": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "redux": "^4.0.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "redux": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/m-asad961/redux-tab-connect.git"
  },
  "bugs": {
    "url": "https://github.com/m-asad961/redux-tab-connect/issues"
  },
  "homepage": "https://github.com/m-asad961/redux-tab-connect#readme"
}